	    $(function(){
	        $("#dosub").click(function(){
	            $('#font_msg').html("");
	            var fieldsetcount = 0;
	            
	            //进行表单检查.检查使用部门 手机 和机器台数
	            var isSignValidate = 1;
	            var alertmsg = "";
	            if ($("#depart_name").val() == "" || $("#depart_name").val() == null) {
	                alertMessage($("#depart_name"), "使用部门不能为空.\n");
	                return false;
	                isSignValidate = 0;
	            }
				
	            if ($("#telephone").val() == "" || $("#telephone").val() == null) {
	                alertMessage($("#telephone"), "手机不能为空.\n");
	                return false;
	                isSignValidate = 0;
	            }
				
				if ($("#idc_id").val() == "" || $("#idc_id").val() == null) {
                    alert("IDC机房必须选择");
                    return false;
                } 
				
				if ($("#vip_type").val()=="1" && $("#VIP").val() == "")
				{
					 alertMessage($("#VIP"), "复用vip模式，则复用vip不能为空.\n");
					return false;
				}
				
				
				var staus_vip = true;
				if ($("#vip_type").val()=="1" && $("#VIP").val() != "")
				{
					var vip = $("#VIP").val();
					$.ajax({
						   type: "POST",
						   url: "/katrina/Ajax/checkvip",
						   data: "VIP="+vip,
						   async: false,
						   dataType: "json",
						   success: function(data){
							   if (data == 0) {
								   alert('vip不存在，请重新输入vip');
								   staus_vip = false;
							   } else {
								   staus_vip = true;
							   }
							   
						   },
						   error: function(){
							   alert("接口错误，稍后再试");
							   staus_vip = false;
						   }
					});
				}
				
				if(!staus_vip){
                	return false;
                }
				
				if ($("#vip_type").val()=="1" && $("#VIP").val() != ""){
		               if (!/^(\d+)\.(\d+)\.(\d+)\.(\d+)$/.test($("#VIP").val())) {
							 alertMessage($("#VIP"), "vip填写不规范.\n");
								return false;
		                }
				}
				
				if ($("#service_name").val() == "") {
                     alertMessage($("#service_name"), "业务名不能为空.\n");
                    return false;
                }
				var RSIP = $("#RSIP").val().split("\n");
				var stat = true;
				$.each(RSIP, function(i) {
	               if (!/^(\d+)\.(\d+)\.(\d+)\.(\d+)$/.test($.trim(RSIP[i]))) {
	                     stat =  false;
	                }
				});
				if (!stat) {
					alertMessage($("#RSIP"), "ip填写不规范.\n");
					alert("ip填写不规范");
					return false;
				}

				if (  $("#vip_port").val() == "") {
                     alertMessage($("#vip_port"), "服务端口不能为空.\n");
                    return false;
                }
				
				if ($("#email_alarm").val() == "") {
                    alertMessage($("#email_alarm"), "邮件报警组不能为空.\n");
                    return false;
                }
				var staus_email_group = true;
				if($("#email_alarm").val() != ""){
					$.ajax({
						type : "POST",
						url  : "/katrina/Ajax/checkAlarmGroupExits",
						data : "alarm_group_name="+$.trim($("#email_alarm").val()),
						async: false,
						   dataType: "json",
						   success: function(data){
							   if (data == 0) {
								   alert('email_alarm不存在，请重新输入email_alarm');
								   staus_email_group = false;
							   } else {
								   staus_email_group = true;
							   }
							   
						   },
						   error: function(){
							   alert("接口错误，稍后再试");
							   staus_email_group = false;
						   }
					});
				}
				
				if(!staus_email_group){
					return false;
				}
				
				if ($("#sms_alarm").val() == "") {
                    alertMessage($("#sms_alarm"), "短信报警组不能为空.\n");
                    return false;
                }
				
				var staus_sms_group = true;
				if($("#sms_alarm").val() != ""){
					$.ajax({
						type : "POST",
						url  : "/katrina/Ajax/checkAlarmGroupExits",
						data : "alarm_group_name="+$.trim($("#sms_alarm").val()),
						async: false,
						   dataType: "json",
						   success: function(data){
							   if (data == 0) {
								   alert('sms_alarm不存在，请重新输入sms_alarm');
								   staus_sms_group = false;
							   } else {
								   staus_sms_group = true;
							   }
							   
						   },
						   error: function(){
							   alert("接口错误，稍后再试");
							   staus_sms_group = false;
						   }
					});
				}
				if(!staus_sms_group){
					return false;
				}
				
				if ($("#checker").val() == "") {
                    alertMessage($("#checker"), "健康检查不能为空.\n");
                   return false;
                }
	            var totalip_type = 0;
	            var need_memo = 0;
	            
	            if (isSignValidate == 0) {
	                alert(alertmsg);
	                return;
	            }
				
				$("#rport").val($("#vip_port").val());
				
	            var ok = confirm("确认表单信息，是否提交？");
	            if (ok == true) {
	                if ($("input:hidden[name='role_type']").val() == 0) {
	                    $("#choose-next").modal("show");
	                    
	                    $("#sub_next_person").click(function(){
	                        var ok = confirm("确认选择此人作为下级审批？");
	                        if (ok) {
	                            var nextPerson = $("select[name='next_exec_person']").val();
	                            $("input:hidden[name='next_exec_peron']").val(nextPerson);
	                            $("#add_task").submit();
	                        } else {
	                            return false;
	                        }
	                    })
	                } else { 
	                    $("#add_task").submit();
	                }
	            }
	        })
	    })
	    
	    function SetSave(){
	        $("#end_flow_flag").val("0");
	        $("#add_task").submit();
	        $("#end_flow_flag").val("1");
	    }
	    
	    $(document).ready(function(){
	    	if ($("#vip_type").val() == 1) {
	    		$("#VIP").show();
    			$("#net_type").hide();
    			$("#vip_tips").html("复用VIP : ");
	    	} else {
	    		$("#VIP").val("");
	    	}
	    	
        	$("#vip_type").change(function(){
        		if ($("#vip_type").val() == 0) {
        			$("#net_type").show();
        			$("#VIP").hide();
        			$("#VIP").val("");
        			$("#vip_tips").html("VIP类型 : ");
        		} else {
        			$("#VIP").show();
        			$("#net_type").hide();
        			$("#vip_tips").html("复用VIP : ");
        		}
        	});
        	
        	
        	if ($("#checker").val() == "") {
    			$("#for_md5_stauts").hide();
    			$("#for_url_path").hide();
    			$("#for_digest").hide();
    			$("#for_status_code").hide();
    			$("#for_rport").hide();
	    	} else {
	    		if (("#checker").val() == "HTTP_GET" || ("#checker").val() == "SSL_GET") {
	    			$("#for_md5_stauts").show();
	    			$("#for_url_path").show();

	    			if ($("input[name='md5_stauts']:checked").val() == 0) {
	        			$("#for_digest").show();
	        			$("#for_status_code").hide();
	    	    	} else {
	    	    		$("#for_digest").hide();
	        			$("#for_status_code").show();
	    	    	}
	    		} else {
	    			$("#for_md5_stauts").hide();
        			$("#for_url_path").hide();
        			$("#for_digest").hide();
        			$("#for_status_code").hide();
	    			$("#for_rport").show();
	    		}
	    	}
        	
        	$("#checker").change(function(){
        		if ($("#checker").val() == "") {
        			$("#for_md5_stauts").hide();
        			$("#for_url_path").hide();
        			$("#for_digest").hide();
        			$("#for_rport").hide();
    	    	} else {
    	    		if ($("#checker").val() == "HTTP_GET" || $("#checker").val() == "SSL_GET") {
    	    			$("#for_md5_stauts").show();
    	    			$("#for_url_path").show();
    	    			$("#for_rport").hide();
    	    			
    	    			if ($("input[name='md5_stauts']:checked").val() == 0) {
    	        			$("#for_digest").show();
    	        			$("#for_status_code").hide();
    	    	    	} else {
    	    	    		$("#for_digest").hide();
    	        			$("#for_status_code").show();
    	    	    	}
    	    		} else {
    	    			$("#for_md5_stauts").hide();
            			$("#for_url_path").hide();
            			$("#for_digest").hide();
            			$("#for_status_code").hide();
    	    			$("#for_rport").show();
    	    		}
    	    	}
        	});
        	
        	$("input[name='md5_stauts']").click(function(){
        		if ($("input[name='md5_stauts']:checked").val() == 0) {
        			$("#for_digest").show();
        			$("#for_status_code").hide();
    	    	} else {
    	    		$("#for_digest").hide();
        			$("#for_status_code").show();
    	    	}
        	});
        	
        	$("#vip_port").blur( function () { 
                $("#rport").val($("#vip_port").val());
            });
        });
    </script>
    

		
