<div class=" row">
    <div class="col-md-3 col-sm-3 col-xs-6">
        <a data-toggle="tooltip" title="分配的机器次" class="well top-block" href="#">
            <i class="glyphicon glyphicon-user blue"></i>

            <div>成员</div>
            <div>[% count.user %]</div>
        </a>
    </div>

    <div class="col-md-3 col-sm-3 col-xs-6">
        <a data-toggle="tooltip" title="项目总数" class="well top-block" href="#">
            <i class="glyphicon glyphicon-star green"></i>

            <div>项目</div>
            <div>[% count.project %]</div>
        </a>
    </div>

    <div class="col-md-3 col-sm-3 col-xs-6">
        <a data-toggle="tooltip" title="被分配的申请" class="well top-block" href="#">
            <i class="glyphicon glyphicon-briefcase yellow"></i>

            <div>机器</div>
            <div>[% count.host %]</div>
        </a>
    </div>

    <div class="col-md-3 col-sm-3 col-xs-6">
        <a data-toggle="tooltip" title="未分配的申请" class="well top-block" href="/project/list_unaskforresources">
            <i class="glyphicon glyphicon-envelope red"></i>

            <div>未分配</div>
            <div>[% count.askforresources %]</div>
        </a>
    </div>
</div>


<div class="row">
    <div class="box col-md-12">
    <div class="box-inner">
    <div class="box-header well" data-original-title="">
        <h2><i class="glyphicon glyphicon-user"></i>ssl证书</h2>

        <div class="box-icon">
            <a href="#" class="btn btn-setting btn-round btn-default"><i class="glyphicon glyphicon-cog"></i></a>
            <a href="#" class="btn btn-minimize btn-round btn-default"><i
                    class="glyphicon glyphicon-chevron-up"></i></a>
            <a href="#" class="btn btn-close btn-round btn-default"><i class="glyphicon glyphicon-remove"></i></a>
        </div>
    </div>



    <div class="box-content">

        <div class="alert alert-info">
 <!--           <form action ="/project/upload_ssl" method=post enctype="multipart/form-data" > -->
<form role="form" id="formid" name= "myform" action ="/project/upload_ssl" method=post enctype="multipart/form-data"  >

            <div class="form-group">
               <table border="1">
               <tr>
               <td><label for="InputFile">上传SSL:</label></td>
               <td><input type="file" id="InputFile" name='filename' ></td>
               <td>

              <button type="submit" class="btn btn-info btn-setting" onclick = "clear_show_info();" >提交</button>
</td>
               </tr>
               </table>
            </div>
            </form>
        </div>

    <table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
    <thead>
    <tr>
        <th>名称</th>
        <th>MD5</th>
        <th>文件</th>
        <th>下载</th>
    </tr>
    </thead>
    <tbody>
[% FOREACH d IN data %]
    <tr>
        <td>[% d.1 %]</td>
        <td>[% d.0 %]</td>
        <td>[% d.2 %]</td>
        <td class="center">
        <a href='/project/download_ssl/[% d.2 %]' target='_blank'  title="下载">

                <i class="glyphicon glyphicon-arrow-down"></i>
</a>
 
        </td>

    </tr>
[% END %]

    </tbody>
    </table>
    </div>
    </div>
